#include<stdio.h>
int main()
{
        int i=1,sum=0,n;
        printf("enter the last value");
        scanf("%d",&n);
        for(i=1; i<=n; i++)
        {
                if (i%2!=0)
                        sum=sum+i;
                i++;
        }
        printf("the sum of odd number is %d",sum);
        return 0;
}